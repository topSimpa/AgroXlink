{
  "expo": {
    "name": "AgroXlink",
    "slug": "AgroXlink",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./apps/assets/splash.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./apps/assets/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./apps/assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./apps/assets/favicon.png"
    },
    "plugins": [
      [

        "expo-font", {
          "fonts": ["./apps/assets/fonts/RedHatDisplay.ttf", "./apps/assets/fonts/RedHatText.ttf"]
        }
      ]
    ]
  }
}
